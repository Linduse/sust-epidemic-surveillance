/*背景颜色*/
.indexContainer{
  flex: auto;
  background-color:#10aeb5;
}
  /*标题*/
  .data-title{
    display: flex;
    color: white;
    flex-direction: column;
    margin-left: 30rpx;
  }
  .context{
    background-color: white;
    width: 95%;
    height: 100%;
    border-style: solid;
    border-color: #eeeeee;
    border-width: 20rpx;
    border-radius: 10rpx;
    margin: 10rpx 20rpx 20rpx 20rpx;
  }
  .statistics {
    width: 100%;
    height: 70rpx;
    display: flex;
    background: white;
    border-bottom: 1rpx solid #eee;
    padding: 20rpx 20rpx 5prx 5rpx;
  }
  .national {
    font-size: 40rpx;
    font-weight: bolder;
    line-height: 70rpx;
  }
  .time {
    font-size: #eee;
    font-size: 20rpx;
    line-height: 90rpx;
    margin-left: 30rpx;
  }
  .data-detail {
    width: 100%;
    height: 600rpx;
    display: flex;
    background-color: white;
    margin:  0rpx 20rpx 30rpx 0;
    padding-left:15rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
  }
  .item {
    width: 45%;
    height: 20%;
    margin: 10rpx;
    text-align: center;
    background: #eee;
    padding: 10rpx
  }
  .nav{
    height: 70rpx;
					display: flex;
					background: white;
					border-bottom: 1rpx solid #eee;
          margin: 10rpx 20rpx 20rpx 20rpx;
          border-radius: 10rpx;
  }
  .icon {
    width: 6rpx;
    background-color: #5555ff;
    margin-right: 20rpx;
  }


  /*地图*/
  .mapButtonContainer{
    display: flex;
    margin: 10rpx 20rpx 20rpx 20rpx;
  }
  .mapContainer{
    background-color: white;
    margin: 10rpx 20rpx 20rpx 20rpx;
  }
  /*折线图*/
  .lineButtonContainer{
    display: flex;
    margin: 10rpx 20rpx 20rpx 20rpx;
  }
  .lineContainer{
    background-color: white;
    margin: 10rpx 20rpx 20rpx 20rpx;
  }
  /*树形表格*/
  .formContainer{
    background-color: white;
    margin: 10rpx 20rpx 20rpx 20rpx;
  }

  .barButtonContainer{
    display: flex;
    margin: 70rpx 20rpx 20rpx 20rpx;
  }
 




  .footContainer{
    height: 100rpx;
    width: 100%;
  }
  


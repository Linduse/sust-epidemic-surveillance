          <view style="font-size: 40rpx;font-weight: bolder;line-height: 40rpx">
            进入{{to}}
          </view>
          <view style="color:{{toColor}};margin-left: 20rpx;height: 41rpx;border: {{toColor}} solid 1rpx;">
            {{toText}}
